package com.viris.debuger;

import java.util.regex.Matcher;
import java.util.regex.Pattern;

import bsh.EvalError;
import bsh.Interpreter;

public class BeanShellTest {

    public static interface InterfaceTest {

        public void sayHello();

    }

    public int getInt(){
    	return 10;
    }
    public static void main(String[] args) {
        try {
        	int a=2;
        	Matcher m = Pattern.compile("\\[(.+?)\\]=(.+?);")
        		     .matcher("[41cfdasfdas818]=new String();[fdas818]=new String(); fdasdsf=bla;[41c5818].toString();[2332fafdkjfda].blabla();");
        		 while (m.find()) {
        			 System.out.println(m.group(0));
        		 }
        		 
        		 Object b = 3;
        		 System.out.println(b.toString());
        		 
            Interpreter interpreter = new Interpreter();
            interpreter.set("object",3032);
            BeanShellTest res = (BeanShellTest) interpreter.eval("import com.viris.debuger.BeanShellTest;" +
                    "BeanShellTest test = new BeanShellTest(); " +
                    "System.out.println(\"hello\"+object); ");
            //res.getInt();
        } catch(EvalError e) {
            e.printStackTrace();
        }
    }

    public void test() {

    }

}
